{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "target": "ES2020",
    "module": "CommonJS",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "noEmit": false,
    "outDir": ".vercel/output",
    "rootDir": "..",
    "baseUrl": "..",
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "./**/*.ts",
    "../data/**/*.ts",
    "../types.ts",
    "../utils/**/*.ts"
  ],
  "exclude": ["node_modules"]
}
